{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via ts-node",
      "runtimeExecutable": "ts-node",
      "args": ["${workspaceFolder}/dist/index.js"],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "npm: build",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
